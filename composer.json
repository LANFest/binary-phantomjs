{
    "name": "vaimo/phantomjs-installer",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "A Composer package which installs the PhantomJS binary (Linux, Windows, Mac) into `bin` of your project",
    "authors": [
        {
            "name": "Jens-AndrÃ© Koch",
            "email": "jakoch@web.de"
        },
        {
            "name": "Allan Paiste",
            "email": "allan.paiste@vaimo.com"
        }
    ],
    "require": {
        "php": ">5.3",
        "ext-openssl": "*",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.2"
    },
    "keywords": ["phantomjs", "headless", "binaries", "downloader", "phantom-installer"],
    "extra": {
        "class": "Vaimo\\PhantomInstaller\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Vaimo\\PhantomInstaller\\": "src"
        }
    }
}
